{
    "include": [
        "app",
        "main.py"
    ],
    "exclude": [
        ".venv",
        "**/.venv",
        "**/venv",
        "**/env",
        "**/__pycache__",
        "**/*.pyc",
        "**/node_modules",
        "**/.git",
        "**/.pytest_cache",
        "**/.mypy_cache"
    ],
    "ignore": [
        ".venv/**",
        "**/.venv/**"
    ],
    "reportMissingImports": true,
    "reportMissingTypeStubs": false,
    "pythonVersion": "3.12",
    "pythonPlatform": "Linux",
    "executionEnvironments": [
        {
            "root": ".",
            "pythonVersion": "3.12",
            "pythonPlatform": "Linux",
            "extraPaths": [
                "./app"
            ]
        }
    ]
}